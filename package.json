{
  "name": "merida",
  "version": "1.0.0",
  "description": "Full stack application",
  "main": "dist/index.js",
  "keywords": [
    "React",
    "NextJS",
    "Apollo",
    "Server",
    "Express",
    "Apollo",
    "Client",
    "Material",
    "UI",
    "NodeJS"
  ],
  "author": "Ajay More",
  "license": "ISC",
  "scripts": {
    "dev": "dotenv nodemon server/index.ts",
    "build": "npm run build:server && npm run build:client && npm run copyfiles",
    "build:server": "rm -rf dist && babel --config-file ./server.babelrc --extensions '.ts,.tsx' ./server --out-dir dist/ && npm run copyfiles && npm run nextbuild",
    "build:client": "dotenv next build",
    "copyfiles": "copyfiles \"./src/**/*.graphql\" \"./src/**/*.json\" \"./src/**/*.ejs\" \"./src/**/*.xlsx\" dist -u 1",
    "start": "dotenv node dist/index.js"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@apollo/react-ssr": "^3.0.1",
    "@date-io/date-fns": "^1.3.9",
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/pickers": "^3.2.3",
    "apollo-boost": "^0.4.4",
    "apollo-link-ws": "^1.0.18",
    "apollo-server-express": "^2.8.2",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "date-fns": "^2.0.0-beta.5",
    "debug": "^4.1.1",
    "dotenv-cli": "^2.0.1",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "formik": "^1.5.8",
    "graphql": "^14.5.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^4.0.0",
    "graphql-type-json": "^0.3.0",
    "helmet": "^3.20.0",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "next": "^9.0.4",
    "next-cookies": "^1.1.2",
    "nexus": "^0.12.0-beta.6",
    "nexus-prisma": "^0.3.7",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prisma-client-lib": "^1.34.6",
    "puppeteer": "^1.19.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-vis": "^1.11.7",
    "session-file-store": "^1.3.1",
    "subscriptions-transport-ws": "^0.9.16",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/next": "^8.0.6",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "copyfiles": "^2.1.1",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "@zeit/next-css": "^1.0.1"
  },
  "nodemonConfig": {
    "watch": [
      "server/**/*.ts"
    ],
    "execMap": {
      "ts": "babel-node --extensions '.ts,.tsx' --config-file ./server.babelrc"
    }
  }
}
